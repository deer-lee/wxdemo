.container{
  padding: 0 26rpx;
}
.manageCon{
  width: 100%;
  display: flex;
  padding: 52rpx 0;
  margin-top: 26rpx;
  align-items: center;
  justify-content: center;
  background: #FFFFFF;
  border: 1rpx solid rgba(219,223,227,0.30);
  box-shadow: 0 5rpx 12rpx 0 rgba(217,226,233,0.50);
  border-radius: 12rpx;
  flex-direction: row;
}

.leftCon{
  width: 142rpx;
  height: 142rpx;
  margin-right: 30rpx;
}
.leftCon image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.rightCon{
  width: 448rpx;
  display: flex;
  flex-direction: column;
}
.rightTitle{
  font-size: 34rpx;
  color: #000000;
  line-height: 30rpx;
  margin-bottom: 39rpx;
}
.rightDesc{
  font-size: 28rpx;
  color: #B8B8B8;
  letter-spacing: 0;
}